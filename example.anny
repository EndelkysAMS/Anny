inicio
    entero x = 5;
    entero y = 10;
    
    si (x < y) entonces
        imprimir("x es menor que y");
    fin_si
    
    mientras (x < y) hacer
        x = x + 1;
    fin_mientras

    para z = 0 hasta 5 con paso 1 hacer
        imprimir(z);
    fin_para
    
    funcion sumar(a, b) hacer
        retorna a + b;
    fin_funcion
    
    entero resultado = sumar(x, y);
    imprimir("El resultado de la suma es: ", resultado);
fin